{
    "name": "machour/yii3-kitchen-sink",
    "type": "project",
    "description": "Exploring Yii3",
    "keywords": [
        "yii",
        "app",
        "project",
        "template"
    ],
    "homepage": "https://github.com/machour/yii3-kitchen-sink",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/machour/yii3-kitchen-sink",
        "issues": "https://github.com/machour/yii3-kitchen-sink/issues"
    },
    "authors": [
        {
            "name": "Mehdi Achour",
            "email": "machour@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev",
        "vlucas/phpdotenv": "^2.5@dev",
        "yiisoft/yii-dataview": "^3.0@dev",
        "league/commonmark": "^0.19.0@dev",
        "ezyang/htmlpurifier": "~4.6",
        "symfony/var-dumper": "^4.3@dev",
        "yiisoft/yii-bootstrap4": "^3.0@dev",
        "yiisoft/yii-jquery": "^3.0@dev",
        "phploc/phploc": "^4",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/rbac": "^3.0@dev",
        "yiisoft/log-target-file": "^3.0@dev",
        "cebe/markdown": "^1.2@dev",
      "ext-json": "*",
      "ext-simplexml": "*"
    },
    "require-dev": {
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "clue/graph-composer": "dev-add-options-to-exclude",
        "yiisoft/yii-debug": "^3.0@dev",
        "yiisoft/yii-gii": "^3.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "params": "config/params.php",
            "web": "config/web.php",
            "console": "config/console.php",
            "dotenv": ".env"
        }
    },
    "autoload": {
        "psr-4": {"app\\": "src"}
    },
    "config": {
        "foxy": {
            "enable-packages": {
                "*": true
            }
        },
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/markuspoerschke/graph-composer"
        }
    ]
}
